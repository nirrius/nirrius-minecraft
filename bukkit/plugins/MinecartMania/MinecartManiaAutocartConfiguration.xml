<?xml version="1.0" encoding="UTF-8"?>
<MinecartManiaConfiguration>
    <version>1.0</version>
    <!--Only minecarts with players in them will be effected by Autocart-->
    <AutocartForPlayersOnly>true</AutocartForPlayersOnly>
    <!--The default throttle percent for minecarts. Must be between 1 and 500.-->
    <DefaultThrottle>150</DefaultThrottle>
</MinecartManiaConfiguration>
